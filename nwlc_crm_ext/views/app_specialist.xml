<?xml version="1.0" encoding="UTF-8"?>



<odoo>
    <record id="dead_app_specialist_view" model="ir.ui.view">
        <field name="name">app.specialist.form</field>
        <field name="model">app.specialist</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="view_app_specialist_tree" model="ir.ui.view">
        <field name="name">app.specialist.tree</field>
        <field name="model">app.specialist</field>
        <field name="arch" type="xml">
            <list >
                <field name="name" />            
            </list>
        </field>
    </record>

    <!-- Define the action -->
    <record id="app_specialist_action" model="ir.actions.act_window">
        <field name="name">App Specialist</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">list,form</field>
        <field name="res_model">app.specialist</field>
        <field name="help" type="html">
            <p class= "h_p">
               
            </p>
        </field>
    </record>
   
    <!-- <menuitem id="app_specialist_menu" 
    name="App Specialist" 
    action="app_specialist_action" 
    parent="crm.crm_menu_config"

    sequence="10"/> -->

</odoo>
