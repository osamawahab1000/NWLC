<odoo>
   <record id="action_csa_general_agreement_template" model="ir.actions.report">
       <field name="name">CSA General Agreement</field>
       <field name="model">crm.lead</field>
       <field name="report_type">qweb-pdf</field>
       <field name="report_name">nwlc_crm_ext.csa_general_agreement</field>
       <field name="report_file">nwlc_crm_ext.csa_general_agreement</field>
       <field name="print_report_name">'Csa General Agreement - %s' % (object.state_id.name if object.state_id else 'N/A')</field>
       <field name="binding_model_id" ref="crm.model_crm_lead"/>
       <field name="binding_type">report</field>
       <field name="paperformat_id" ref="nwlc_crm_ext.paperformat_csa_general"/>
   </record>
   
   
   
   
